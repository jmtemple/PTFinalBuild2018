<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.proyectotiti.structuresCook">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/proyectotiti"
        android:layout_centerHorizontal="true"
        android:paddingTop="0dp"
        tools:ignore="ContentDescription"/>


    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MADERA DEL BOSQUE"
        android:textSize="18sp"
        android:paddingTop="0dp"
        android:layout_below="@+id/logo"
        android:layout_centerHorizontal="true" />

    <!--<Button-->
    <!--android:id="@+id/continueButton"-->
    <!--style="@android:style/Widget.Button"-->
    <!--android:layout_width="150dp"-->
    <!--android:layout_height="52dp"-->
    <!--android:text="SIGUENTE"-->
    <!--android:textColorLink="@color/colorPrimaryDark"-->
    <!--android:layout_alignParentBottom="true"-->
    <!--android:layout_alignParentRight="true"-->
    <!--android:layout_alignParentEnd="true"-->
    <!--tools:layout_editor_absoluteY="460dp"-->
    <!--tools:layout_editor_absoluteX="234dp" />-->

    <!--<Button-->
    <!--android:id="@+id/backButton"-->
    <!--style="@android:style/Widget.Button"-->
    <!--android:layout_width="150dp"-->
    <!--android:layout_height="52dp"-->
    <!--android:text="ANTERIOR"-->
    <!--android:textColorLink="@color/colorPrimaryDark"-->
    <!--android:layout_alignParentBottom="true"-->
    <!--android:layout_alignParentLeft="true"-->
    <!--android:layout_alignParentStart="true"-->
    <!--tools:layout_editor_absoluteY="460dp"-->
    <!--tools:layout_editor_absoluteX="0dp"-->
    <!--android:onClick="openMadera0"/>-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/linearLayout"
        android:orientation="horizontal"
        >

        <Button
            android:id="@+id/backButton"
            style="@android:style/Widget.Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:onClick="openMadera4"
            android:text="ANTERIOR"
            android:textColorLink="@color/colorPrimaryDark" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <Button
            android:id="@+id/continueButton"
            style="@android:style/Widget.Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:onClick="submitStructure"
            android:text="SIGUENTE"
            android:textColorLink="@color/colorPrimaryDark" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/linearLayout"
        android:layout_below="@+id/title"
        android:layout_centerHorizontal="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textViewCookingQ5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="0dp"
                android:text="多Con que frequencia de lena?"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="normal"
                android:visibility="gone"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Spinner
                android:id="@+id/spinnerFreq"
                android:layout_width="275dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.52"
                android:visibility="gone"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textViewCookingQ6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="20dp"
                android:text="多En que tipo de estufa?"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="normal"
                android:visibility="gone"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Spinner
                android:id="@+id/spinnerStoveType"
                android:layout_width="275dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.52"
                android:ems="10"
                android:inputType="textPersonName"
                android:visibility="gone"/>


        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textViewStoveType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Otro"
                    android:textColor="#000000"
                    android:textSize="16sp"
                    android:textStyle="normal"
                    android:visibility="gone"/>
                <EditText
                    android:id="@+id/editTextStoveType"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textViewCookingQ52"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="0dp"
                android:text="多Con que frequencia de carbon?"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="normal"
                android:visibility="gone"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Spinner
                android:id="@+id/spinnerFreq2"
                android:layout_width="275dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.52"
                android:visibility="gone"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textViewCookingQ62"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="20dp"
                android:text="多En que tipo de estufa?"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="normal"
                android:visibility="gone"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Spinner
                android:id="@+id/spinnerStoveType2"
                android:layout_width="275dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.52"
                android:ems="10"
                android:inputType="textPersonName"
                android:visibility="gone"/>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textViewStoveType2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Otro"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="normal"
                android:visibility="gone"/>
            <EditText
                android:id="@+id/editTextStoveType2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"/>


        </LinearLayout>

    </LinearLayout>

</RelativeLayout>
